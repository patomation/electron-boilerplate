{
  "name": "electron-boilerplate",
  "version": "2.0.0",
  "description": "A minimal Electron application",
  "main": "./prod.main.js",
  "scripts": {
    "start": "concurrently -k -n \"WEBPACK,ELECTRON\" -c \"blue,magenta\" \"npm run webpack:start\" \"npm run electron:start\"",
    "electron:start": "wait-on http://localhost:3000 && electron ./dev.main.js",
    "webpack:start": "webpack-dev-server --mode development --hot",
    "webpack:build": "webpack --mode production",
    "package:zip": "node packageApp.js --zip",
    "package": "node packageApp.js",
    "build": "npm run webpack:build && npm run package:zip",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "boilerplate",
    "webpack"
  ],
  "author": "Patrick Kelly",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "7.21.4",
    "acorn": "8.8.2",
    "babel-loader": "9.1.2",
    "concurrently": "8.0.1",
    "css-loader": "6.7.3",
    "electron": "24.0.0",
    "electron-packager": "17.1.1",
    "electron-reload": "1.5.0",
    "eslint": "8.38.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-standard": "4.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "node-sass": "8.0.0",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "wait-on": "7.0.1",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2"
  },
  "private": true
}
