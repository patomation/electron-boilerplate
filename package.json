{
  "name": "electron-boilerplate",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./src/prod.main.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run sass \" \"npm run dev\"",
    "dev": "electron ./src/dev.main.js",
    "sass": "node-sass --watch ./src/sass -o ./src/css",
    "build-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron Boilerplate'",
    "build-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "build-mac": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron Boilerplate'"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.4",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "node-sass": "^4.12.0"
  },
  "dependencies": {
    "remote": "^0.2.6"
  }
}
