{
  "name": "electron-boilerplate",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "./prod.main.js",
  "scripts": {
    "start": "concurrently -k -n \"WEBPACK,ELECTRON\" -c \"blue,magenta\" \"yarn webpack-start\" \"yarn electron-start\"",
    "electron-start": "wait-on http://localhost:8080 && electron ./dev.main.js",
    "webpack-start": "webpack-dev-server --mode development --hot",
    "webpack-build": "webpack --mode production",
    "package-win": "electron-packager . electron-boilerplate-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron Boilerplate'",
    "package-linux": "electron-packager . electron-boilerplate-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-mac": "electron-packager . electron-boilerplate-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Electron Boilerplate'",
    "build-win": "yarn webpack-build && yarn package-win",
    "build-linux": "yarn webpack-build && yarn package-linux",
    "build-mac": "yarn webpack-build && yarn package-mac"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "boilerplate"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "concurrently": "^4.1.0",
    "css-loader": "^3.0.0",
    "electron": "^5.0.4",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "wait-on": "^3.2.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "acorn": "^7.0.0"
  },
  "dependencies": {
    "remote": "^0.2.6"
  },
  "private": true
}
