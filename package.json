{
  "name": "electron-boilerplate",
  "version": "0.1.2",
  "description": "A minimal Electron application",
  "main": "./prod.main.js",
  "scripts": {
    "start": "concurrently -k -n \"WEBPACK,ELECTRON\" -c \"blue,magenta\" \"npm run webpack:start\" \"npm run electron:start\"",
    "electron:start": "wait-on http://localhost:3000 && electron ./dev.main.js",
    "webpack:start": "webpack-dev-server --mode development --hot",
    "webpack:build": "webpack --mode production",
    "package:zip": "node packageApp.js --zip",
    "package": "node packageApp.js",
    "build": "npm run webpack:build && npm run package:zip",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "boilerplate",
    "webpack"
  ],
  "author": "Patrick Kelly",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "7.10.5",
    "acorn": "7.3.1",
    "babel-loader": "8.1.0",
    "concurrently": "5.2.0",
    "css-loader": "3.6.0",
    "electron": "13.6.6",
    "electron-packager": "15.0.0",
    "electron-reload": "1.5.0",
    "eslint": "7.4.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "node-sass": "4.14.1",
    "sass-loader": "9.0.2",
    "style-loader": "1.2.1",
    "wait-on": "5.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "private": true
}
